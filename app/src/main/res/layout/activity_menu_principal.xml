<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#272727"
    android:foregroundTint="#00FFFFFF"
    android:stateListAnimator="@anim/nav_default_enter_anim"
    tools:context=".Menu_principal">
    <ImageView
        android:id="@+id/imageView31"
        android:layout_width="416dp"
        android:layout_height="274dp"
        android:layout_marginStart="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.035"
        app:srcCompat="@drawable/s_p_ration_barresiah" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="216dp"
        android:layout_height="184dp"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.071"
        app:srcCompat="@drawable/siahlogo_decoupe3" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="226dp"
        android:layout_height="45dp"
        android:text="Bienvenue "
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#E3E1E1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.06"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.315"
        tools:text="Bienvenue" />

    <TextView
        android:id="@+id/text_date"
        android:layout_width="138dp"
        android:layout_height="42dp"
        android:text="TextView"
        android:textColor="#E3E1E1"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.04"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.378" />

    <Button
        android:id="@+id/acces_podometre"
        android:layout_width="69dp"
        android:layout_height="63dp"
        android:background="#00FFFFFF"
        android:shadowColor="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.271"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageView
        android:id="@+id/imageView32"
        android:layout_width="378dp"
        android:layout_height="85dp"
        android:scaleType="centerCrop"
        android:scaleX="1.55"
        android:scaleY="1.55"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.484"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/barre_selection2" />

    <Button
        android:id="@+id/sport_buttton"
        android:layout_width="68dp"
        android:layout_height="73dp"
        android:backgroundTint="#00FFFFFF"
        android:shadowColor="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.752"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <HorizontalScrollView
        android:layout_width="404dp"
        android:layout_height="304dp"
        android:layout_marginStart="4dp"
        android:layout_marginEnd="3dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView31"
        app:layout_constraintVertical_bias="0.138"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:orientation="vertical"/>
    </HorizontalScrollView>

    <Button
        android:id="@+id/nous"
        android:layout_width="50dp"
        android:layout_height="39dp"
        android:background="@android:color/transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.524"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.976" />

    <Button
        android:id="@+id/btn_health"
        android:layout_width="79dp"
        android:layout_height="54dp"
        android:backgroundTint="#00FFFFFF"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="668dp" />

    <Button
        android:id="@+id/btn_profil"
        android:layout_width="71dp"
        android:layout_height="54dp"
        android:backgroundTint="#00FFFFFF"
        tools:layout_editor_absoluteX="336dp"
        tools:layout_editor_absoluteY="667dp" />


</android.support.constraint.ConstraintLayout>